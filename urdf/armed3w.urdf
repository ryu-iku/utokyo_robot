<?xml version="1.0" encoding="UTF-8"?>
<robot name="armed3w">

  <link name="base_footprint"/>

  <!-- Robot Body -->
  <joint name="base_link_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="body"/>
    <origin xyz="-4 5 0"/>
  </joint>
  <link name='body'>
    <inertial>
      <mass value="1"/>
      <!-- #cylinder Izz = 1/2 * M * r^2
      #cylinder Iyy = Ixx = 1/12 * M * l^2 -->
      <inertia ixx=".0675"  ixy="0"  ixz="0" iyy=".0675" iyz="0" izz=".045"/>
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius=".3" length=".9"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius=".3" length=".9"/>
      </geometry>
    </visual>
  </link>

  <joint name="body_wheelFront" type="fixed">
    <parent link="body"/>
    <child link="wheelFront"/>
    <origin xyz="0 .2 -.48" rpy="0 0 0"/>
  </joint>

  <link name='wheelFront'>
    <inertial>
      <mass value=".2"/>
      <!-- # sphere: I = 2/5 * M * a^2 -->
      <inertia ixx=".0002"  ixy="0"  ixz="0" iyy=".0002" iyz="0" izz=".0002"/>
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".05"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".05"/>
      </geometry>
    </visual>
  </link>

  <joint name="body_wheelRight" type="continuous">
    <parent link="body"/>
    <child link="wheelRight"/>
    <origin xyz="0.1732 -.1 -.48" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <link name='wheelRight'>
    <inertial>
      <mass value=".2"/>
      <!-- # sphere: I = 2/5 * M * r^2 -->
      <inertia ixx=".0002"  ixy="0.0"  ixz="0.0" iyy=".0002" iyz="0.0" izz=".0002"/>
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".05"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".05"/>
      </geometry>
    </visual>
  </link>

  <joint name="body_wheelLeft" type="continuous">
    <parent link="body"/>
    <child link="wheelLeft"/>
    <origin xyz="-0.1732 -.1 -.48" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <link name='wheelLeft'>
    <inertial>
      <mass value=".2"/>
      <!-- # sphere: I = 2/5 * M * r^2 -->
      <inertia ixx=".0002"  ixy="0.0"  ixz="0.0" iyy=".0002" iyz="0.0" izz=".0002"/>
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".05"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".05"/>
      </geometry>
    </visual>
  </link>

  <joint name="body_armRight" type="continuous">
    <parent link="body"/>
    <child link="armRight"/>
    <origin xyz=".3 0 .2" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <link name='armRight'>
    <inertial>
      <mass value=".1"/>
      <!-- #cylinder Izz = 1/2 * M * r^2 -->
      <!-- #cylinder Iyy = Ixx = 1/12 * M * l^2 -->
      <inertia ixx=".003"  ixy="0"  ixz="0" iyy=".003" iyz="0" izz=".000045"/>
    </inertial>
    <collision>
      <origin xyz="0 0 -.3" rpy="0 0 0"/>
      <geometry>
        <cylinder radius=".03" length=".6"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 -.3" rpy="0 0 0"/>
      <geometry>
        <cylinder radius=".03" length=".6"/>
      </geometry>
    </visual>
  </link>

  <joint name="body_armLeft" type="continuous">
    <parent link="body"/>
    <child link="armLeft"/>
    <origin xyz="-.3 0 .2" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <link name='armLeft'>
      <inertial>
        <mass value=".1"/>
      <!-- #cylinder Izz = 1/2 * M * r^2
      #cylinder Iyy = Ixx = 1/12 * M * l^2 -->
      <inertia ixx=".003"  ixy="0"  ixz="0" iyy=".003" iyz="0" izz=".000045"/>
      </inertial>
      <collision>
        <origin xyz="0 0 -.3" rpy="0 0 0"/>
        <geometry>
          <cylinder radius=".03" length=".6"/>
        </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 -.3" rpy="0 0 0"/>
        <geometry>
          <cylinder radius=".03" length=".6"/>
        </geometry>
      </visual>
    </link>

  <!-- controller start-->
  <transmission name="body_wheelRight_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="body_wheelRight">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="body_wheelRight_motor">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>30</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="body_wheelLeft_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="body_wheelLeft">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="body_wheelLeft_motor">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>30</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="body_armRight_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="body_armRight">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="body_armRight_motor">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>30</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="body_armLeft_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="body_armLeft">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="body_armLeft_motor">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>30</mechanicalReduction>
    </actuator>
  </transmission>
  <!-- controller end-->

  <!-- coefficient of friction start-->
  <gazebo reference="body">
    <selfCollide>true</selfCollide>
      <mu1 value="0.05"/>
      <mu2 value="0.05"/>
  </gazebo>

  <gazebo reference="wheelFront">
    <selfCollide>true</selfCollide>
      <mu1 value="0.0"/>
      <mu2 value="0.0"/>
  </gazebo>

  <gazebo reference="wheelRight">
    <selfCollide>true</selfCollide>
      <mu1 value="0.8"/>
      <mu2 value="0.8"/>
  </gazebo>

  <gazebo reference="wheelLeft">
    <selfCollide>true</selfCollide>
      <mu1 value="0.8"/>
      <mu2 value="0.8"/>
  </gazebo>

  <gazebo reference="armRight">
    <selfCollide>true</selfCollide>
      <mu1 value="0.8"/>
      <mu2 value="0.8"/>
  </gazebo>
  <gazebo reference="armLeft">
    <selfCollide>true</selfCollide>
      <mu1 value="0.8"/>
      <mu2 value="0.8"/>
  </gazebo>
  <!-- coefficient of friction end-->


  <joint name="camera_joint" type="fixed">
    <origin xyz="0 -.3 0" rpy="0 0 0"/>
    <parent link="body"/>
    <child link="camera"/>
  </joint>

  <link name="camera">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".1 .1 .1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".1 .1 .1"/>
      </geometry>
      <material name="green"/>
    </visual>

    <inertial>
      <mass value=".01" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <box_inertia m=".01" x=".1" y=".1" z=".1" />
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>


  <joint name="hokuyo_joint" type="fixed">
    <origin xyz="0 -.3 -.1" rpy="0 0 -1.570796"/>
    <parent link="body"/>
    <child link="hokuyo"/>
  </joint>

  <!-- Hokuyo Laser -->
  <link name="hokuyo">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>


  <!-- sensor start-->
  <gazebo reference="camera">
    <material>Gazebo/Green</material>
    <sensor type="camera" name="camera1">
      <update_rate>10</update_rate>
      <camera name="head">
        <horizontal_fov>1.3962634</horizontal_fov>
        <image>
          <width>20</width>
          <height>20</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>0.0</updateRate>
        <cameraName>mybot/camera1</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>camera</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>

  <!-- hokuyo -->
  <gazebo reference="hokuyo">
    <material>Gazebo/FlatBlack</material>
    <sensor type="ray" name="lds_lfcd_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>100</samples>
            <resolution>1</resolution>
            <min_angle>-1.570796</min_angle>
            <max_angle>1.570796</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.10</min>
          <max>20</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>scan</topicName>
        <frameName>hokuyo</frameName>
      </plugin>
    </sensor>
  </gazebo>
  <!-- sensor end-->

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/armed3w</robotNamespace>
    </plugin>
  </gazebo>

</robot>

